<%- include layouts/header.ejs %>

<div class="page-data">

    <form method="post" action="/vendors/add">
        <table cellpadding="11">
            <tr>
                <td class="label">Vendor ID</td><td>: <input type="text" name="vendor_id" required></td>
            </tr>
            <tr>
                <td class="label">Product-Id</td><td>:<input type="text" name="product_id" ></td>
            </tr>
            <tr>
                <td class="label">Vendor's Name<</td><td>: <input type="text" name="vendor_name" required></td>
            </tr>
            <tr>
                <td class="label">Vendor's Address</td><td>: <input type="text" name="vendor_address" required></td>
            </tr>
            <tr>
                <td class="label">Vendor's Phone</td><td>: <input type="text" name="vendor_phone" required></td>
            </tr>
            <tr>
                <td class="label">Vendor's Price</td><td>: <input type="text" name="vendor_price" required></td>
            </tr>
            <tr>
                <td class="label"></td>
                <td>
                    &nbsp; <input type="submit" name="submit" value="Save">
                    <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelVendors()">
                </td>
            </tr>
        </table>
    </form>

</div>

