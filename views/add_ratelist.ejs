<%- include layouts/header.ejs %>

<div class="page-data">

    <form method="post" action="/ratelist/add">
        <table cellpadding="11">
            <tr>
                <td class="label">Product Id</td><td>: <input type="text" name="product_id" required></td>
            </tr>
            <tr>
                <td class="label">Vendor's Id</td><td>: <input type="text" name="vendor_id" required></td>
            </tr>
            <tr>
                <td class="label"></td>
                <td>
                    &nbsp; <input type="submit" name="submit" value="Save">
                    <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelRatelist()">
                </td>
            </tr>
        </table>
    </form>

</div>

